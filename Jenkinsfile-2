pipeline {
  agent any
  environment {
   ANSIBLE_PRIVATE_KEY=credentials('id_rsa') 
  }
  stages {
    stage('Hello') {
      steps {
        sh 'ansible-galaxy collection install -r requirements.yml'
        sh 'ansible-playbook -i inventory/mariadb.hosts -K playbooks/mariadb.yml'
      }
    }
  }
}
